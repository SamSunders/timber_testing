      </script>
      <noscript>
          <iframe title="DoubleClick Floodlight Tag" src="https://2549153.fls.doubleclick.net/activityi;src=2549153;type=entga0;cat=ent_a02u;ord=1?" width="1" height="1" frameborder="0" style="display:none"></iframe>
      </noscript>
      <!-- End of DoubleClick Floodlight Tag: Please do not remove -->

      <div class="site-wrapper">

        <header class="header main {{header_class}}">
            <a href="/">
              <img src="{{ site.theme.link }}/images/Wells-Fargo-Logo.svg" alt="Wells Fargo Logo" />
              <div class="brand">
                Wells Fargo Stories
                <span>An online journal of working together.</span>
              </div>
            </a>
            <div class="menu-expander">
                <a href="#" class="menu-open">
                <span class="menu-text">Menu</span>
                <span class="dashicons dashicons-menu"></span>
                </a>
            </div>
            
        </header>

        <div class="nav-wrapper">
          <nav>
            <div class="nav-header">
                <a href="#" class="menu-close">
                <div class="menu-closer">
                    <span class="menu-text">Close</span>
                    <span class="dashicons dashicons-no-alt"></span>
                </div>
                </a>
                <div class="clear-fix"></div>
            </div>
            <div class="nav-container">
                <a href="/category/helping-our-customers-succeed/" class="category helping-succeed">Helping Customers</a>
                <div class="sub-category">
                    <a href="/category/innovation/">Innovation</a>
                    <a href="/category/small-business/">Small Business</a>
                    <a href="/category/homeownership/">Homeownership</a>
                    <a href="/category/financial-health/">Financial Health</a>
                    <a href="/category/business-to-business/">Business to Business</a>
                </div>
                <a href="/category/helping-our-communities-thrive/" class="category helping-thrive">Helping Communities</a>
                <div class="sub-category">
                    <a href="/category/volunteering-giving/">Volunteering & Giving</a>
                    <a href="/category/economic-opportunity/">Economic Opportunity</a>
                    <a href="/category/environment/">Environment</a>
                </div>
                <a href="/category/living-our-values/" class="category living-values">Living Our Values</a>
                <div class="sub-category">
                    <a href="/category/diversity-inclusion/">Diversity & Inclusion</a>
                    <a href="/category/our-people/">Our People</a>
                    <a href="/category/inside-the-stagecoach/">Inside the Stagecoach</a>
                    <a href="/category/our-history/">Our History</a>
                </div>
                <a href="/about/" class="category about">About</a>
                <div class="search-form">
                    <form id="search-form" name="search-form" action="/" method="get" role="search">
                        <label for="search">Search</label>
                        <input id="search" type="text" name="s" placeholder="Search">
                            <i class="fa fa-search search-icon"></i>
                        </input>
                    </form>
                </div>
                <p>Subscribe to Stories</p>
                <form class="subscribe-form">
                  <input type="text" placeholder="Enter your email address" class="subscribe-email" aria-label="Enter your email address">
                  <input type="submit" value="Subscribe" class="subscribe-btn">
                </form>
            </div>
          </nav>
        </div>

        {# Main Content #}
        {% block content %}
            {# template specific content goes here #}
        {% endblock %}

        <footer>
            <div class="wrapper">
            {# Footer #}
            {% block footer %}
                {# footer template goes here #}
                {% include 'footer.twig' ignore missing %}
            {% endblock %}
            </div>
        </footer>


        <div class="modal modal-hide" id="externalConfirm">
            <div class="modal-fade-screen">
                <div class="modal-inner">
                    <div class="modal-header">
                        <img class="wf-logo" src="{{ site.theme.link }}/images/Wells-Fargo-Logo.svg" alt="Wells Fargo Logo" />
                        <h1>You are leaving the Wells Fargo Stories website</h1>
                        <div class="modal-close" for="modal-1"></div>
                    </div>
                    <p class="modal-intro"></p>
                    <p class="modal-content">You are leaving wellsfargo.com/stories and
                    entering a website that Wells Fargo does not control. Wells Fargo has
                    provided this link for your convenience, but does not endorse and is
                    not responsible for the content, links, privacy policy, or security
                    policy of this website.</p>
                    <a href="#" class="home-more-btn continue" target="_blank">Continue</a>
                    <a href="#" class="home-more-btn cancel">Cancel</a>
                </div>
            </div>
        </div>

      </div><!-- /wrapper -->


      <script type="text/javascript" src="//code.jquery.com/jquery-1.11.2.min.js"></script>
      <script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
      <script type="text/javascript" src="/wp-includes/js/mediaelement/mediaelement-and-player.min.js"></script>
      <script type="text/javascript" src="{{ site.theme.link }}/mediaelement/wp-mediaelement.js"></script>
      <script type="text/javascript" src="{{ site.theme.link }}/scripts/slick.min.js"></script>
      <script type="text/javascript" src="{{ site.theme.link }}/scripts/fancybox.min.js"></script>
      <script type="text/javascript" src="{{ site.theme.link }}/scripts/main.js"></script>
      <script type="text/javascript" src="{{ site.theme.link }}/scripts/subscribe.js"></script>

      {# Javascript Footer #}
      {{ function('wp_footer') }}

      {# Custom Javascript #}
      {% block template_js %}
          {# template specific javascript goes here #}
      {% endblock %}

      <!-- Adobe Analytics code version: H.26.2 -->
      <script type="text/javascript" src="{{ site.theme.link }}/scripts/s_code.js"></script>
      <script type="text/javascript">
          <!-- /* You may give each page an identifying name, channel, site levels on the next lines.*/
          {% if post.title %}
          s.pageName="{{post.title}}"; //must set this, and must be unique for each page
          {% else %}
          s.pageName="{{function('wp_title', '|', true, 'right')}}"; //must set this, and must be unique for each page
          {% endif %}
          {% if post.category.parent %}
          {% set parent_category = TimberTerm(post.category.parent) %}
          s.hier1 = "{{parent_category}} | {{post.category}}";
          {% endif %}
          {#
          //s.channel = ""; //can set channel
          //can set site section levels 1-4 with these
          //s.prop1="";
          //s.prop2="";
          //s.prop3="";
          //s.prop4="";
          #}

          /************* DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/
          var s_code=s.t();if(s_code)document.write(s_code)//-->
      </script>
